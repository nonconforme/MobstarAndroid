<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="progressButtonStyle" format="reference" />

    <declare-styleable name="ProgressButton">
        <attr name="progress" format="integer" />
        <attr name="max" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="pinnedDrawable" format="reference" />
        <attr name="unpinnedDrawable" format="reference" />
        <attr name="shadowDrawable" format="reference" />
        <attr name="innerSize" format="dimension" />
        <attr name="pinned" format="boolean" />
        <attr name="animating" format="boolean" />
        <attr name="animationSpeed" format="integer" />
        <attr name="animationDelay" format="integer" />
        <attr name="animationStripWidth" format="integer" />
        <attr name="android:background" />
        <attr name="android:clickable" />
        <attr name="android:focusable" />
    </declare-styleable>
    
   <declare-styleable name="ProgressWheel">
        <attr name="p_text" format="string" />
        <attr name="p_textColor" format="color" />
        <attr name="p_textSize" format="dimension" />
        <attr name="p_barColor" format="color" />
        <attr name="p_rimColor" format="color" />
        <attr name="p_rimWidth" format="dimension" />
        <attr name="p_spinSpeed" format="dimension" />
        <attr name="p_delayMillis" format="integer" />
        <attr name="p_barWidth" format="dimension" />
        <attr name="p_barLength" format="dimension" />
        <attr name="p_circleColor" format="color" />
        <attr name="p_radius" format="dimension" />
    </declare-styleable> 
    
   	<declare-styleable name="ProgressWheelPercentage">
        <attr name="text" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="per_circleColor" format="color" />
        <attr name="per_radius" format="dimension" />
        <attr name="barWidth" format="dimension" />
        <attr name="barLength" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CheckableView">
        <attr name="isChowChecked" format="boolean" />
        <attr name="checkedTitle" format="string"/>
        <attr name="leftImage" format="reference"/>
    </declare-styleable>
    
   

</resources>